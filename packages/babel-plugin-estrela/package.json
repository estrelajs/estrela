{
  "name": "babel-plugin-estrela",
  "description": "A babel plugin to preprocess estrela jsx/tsx files",
  "author": "Eduardo Rosostolato",
  "version": "0.10.0",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "babel",
    "babel-plugin",
    "babel-plugin-estrela",
    "estrela",
    "jsx",
    "tsx"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile && tsc",
    "clean": "rimraf dist",
    "compile": "esbuild $(esbuild-config)",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@babel/core": "^7.18.5",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "postcss-prefix-selector": "^1.15.0"
  },
  "devDependencies": {
    "@babel/helper-annotate-as-pure": "^7.16.7",
    "@babel/helper-plugin-utils": "^7.17.12",
    "@types/babel__core": "^7.1.19",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@types/postcss-prefix-selector": "^1.15.0",
    "esbuild": "^0.14.43",
    "esbuild-config": "^0.2.0",
    "rimraf": "^3.0.2"
  }
}