{
  "name": "vite-plugin-estrela",
  "description": "A vite plugin to pre-process estrela jsx/tsx files",
  "author": "Eduardo Rosostolato",
  "version": "0.11.2",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn compile && tsc",
    "clean": "rimraf dist",
    "compile": "esbuild $(esbuild-config)",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "estrela",
    "jsx",
    "tsx"
  ],
  "dependencies": {
    "@babel/core": "^7.19.1",
    "babel-plugin-estrela": "^0.11.2"
  },
  "devDependencies": {
    "@types/node": "^18.7.19",
    "esbuild": "^0.15.9",
    "esbuild-config": "^1.0.1",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "vite": "^3.1.3"
  }
}