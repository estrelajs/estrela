{
  "name": "vite-plugin-estrela",
  "description": "A vite plugin to process estrela jsx/tsx files",
  "author": "Eduardo Rosostolato",
  "version": "0.3.1",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn compile && tsc",
    "clean": "rimraf dist",
    "compile": "esbuild $(esbuild-config)",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "estrela",
    "jsx",
    "tsx"
  ],
  "dependencies": {
    "@babel/core": "^7.18.0",
    "babel-plugin-estrela": "^0.3.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "esbuild": "^0.14.39",
    "esbuild-config": "^0.2.0",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "vite": "^2.9.9"
  }
}
