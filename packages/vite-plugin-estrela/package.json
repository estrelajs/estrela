{
  "name": "vite-plugin-estrela",
  "version": "1.4.1",
  "description": "A vite plugin to process estrela jsx/tsx files",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "yarn clean && yarn compile && tsc",
    "clean": "rimraf dist",
    "compile": "esbuild src/index.ts --bundle --platform=node --external:@babel/core --external:@babel/types --external:vite --outfile=dist/index.js",
    "prepublish": "yarn build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/babel__core": "^7.1.19",
    "@types/node": "^17.0.24",
    "babel-plugin-estrela": "^0.1.0",
    "esbuild": "^0.14.38",
    "rimraf": "^3.0.2"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0",
    "vite": "^2.9.0"
  }
}